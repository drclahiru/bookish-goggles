let id = (x) {
    x
}

let apply ((Number) -> Number, Number) -> Number = (f, x) {
    f(x)
}

let apply2 = (f, x) {
    f(x)
}

let a Number = 2

let b = apply(square, a)

let c = apply2(id, square)

let square = (x) {
    x * x
}

let fact = (n) {
    if n == 0 {
        1
     } else {
        n * fact(n - 1)
     }
}

